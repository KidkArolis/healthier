// this is using eslint-config-prettier to turn off
// styling related standard rules and elint-plugin-prettier
// to lint prettier style rules
{
  "extends": [
    "standard",
    "standard-jsx",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/babel",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard",
    "prettier/unicorn",
    "prettier/vue"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
